{
  "version": 2,
  "builds": [{ "src": "app.py", "use": "@vercel/python" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "SECRET_KEY": "@secret_key",
    "DATABASE_URL": "@database_url",
    "SECURITY_PASSWORD_SALT": "@security_password_salt"
  },
  "functions": {
    "app.py": {
      "maxDuration": 10
    }
  }
}